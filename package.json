  },
  "homepage": "https://github.com/rodgars/bigball#readme",
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "concurrently": "^7.4.0",
    "connect-redis": "^6.1.3",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "g": "^2.0.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.6.2",
