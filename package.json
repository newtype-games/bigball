{
  "name": "bigball",
  "version": "1.0.0",
  "description": "Have fun with friends and enjoy World Cup!",
  "main": "index.js",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
